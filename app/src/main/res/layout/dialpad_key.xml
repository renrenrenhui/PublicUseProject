<?xml version="1.0" encoding="utf-8"?><!-- A layout representing a single key in the dialpad -->
<com.example.xiaoniu.publicuseproject.dial.DialpadKeyButton xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/DialpadKeyButtonStyle">

    <LinearLayout
        android:id="@+id/dialpad_key_layout"
        style="@style/DialpadKeyInternalLayoutStyle">

        <com.example.xiaoniu.publicuseproject.dial.DialpadTextView
            android:id="@+id/dialpad_key_number"
            style="@style/DialpadKeyNumberStyle" />

        <TextView
            android:id="@+id/dialpad_key_letters"
            style="@style/DialpadKeyLettersStyle" />
    </LinearLayout>
</com.example.xiaoniu.publicuseproject.dial.DialpadKeyButton>
